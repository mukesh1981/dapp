<script>
// Wait for the page to load
window.addEventListener('load', () => {
  // Wait for 3 seconds and show the cookie consent modal
  setTimeout(() => {
    const cookieConsentModal = new bootstrap.Modal(document.getElementById('cookieconsent3'))
    cookieConsentModal.show(),
  }, 6000)
})

// Listen for the cookie consent form submit event
const cookieConsentForm = document.getElementById('cookie-consent-form')
cookieConsentForm.addEventListener('submit', (event) => {
  event.preventDefault()

  // Get the selected option and store it in a cookie for 3 months
  const consentOption = cookieConsentForm.elements['consent'].value
  const expiryDate = new Date()
  expiryDate.setTime(expiryDate.getTime() + (90 * 24 * 60 * 60 * 1000)) // 90 days
  document.cookie = `cookie_consent=${consentOption};expires=${expiryDate.toUTCString()};path=/`

  // Hide the cookie consent modal
  const cookieConsentModal = bootstrap.Modal.getInstance(document.getElementById('cookieconsent3'))
  cookieConsentModal.hide()
})

// Check if the user has already given cookie consent
function hasCookieConsent() {
  const cookieValue = `; ${document.cookie}`
  const parts = cookieValue.split(`; cookie_consent=`)
  if (parts.length === 2) {
    const cookieOption = parts.pop().split(';').shift()
    return cookieOption === 'necessary' || cookieOption === 'analytical' || cookieOption === 'marketing'
  }
  return false
}

// Hide the cookie consent modal if the user has already given consent
if (hasCookieConsent()) {
  const cookieConsentModal = bootstrap.Modal.getInstance(document.getElementById('cookieconsent3'))
  cookieConsentModal.hide()
}
</script>
