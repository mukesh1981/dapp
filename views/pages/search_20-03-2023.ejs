<script>
  const cookieConsent = new CookieConsent({
    contentUrl: "/node_modules/cookie-consent-js/cookie-consent-content", // location of the language files
    privacyPolicyUrl: "/privacy-policy.html"
})
//cookieSettings.showDialog()
var cookieConsent = new CookieConsent({
    autoShowModal: false,
    privacyPolicyUrl: "privacy-policy.html",
    contentUrl: "./cookie-consent-content"
})
// function to set a cookie
function setCookie(name, value, days) {
  var expires = "";
  if (days) {
    var date = new Date();
    date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000);
    expires = "; expires=" + date.toUTCString();
  }
  document.cookie = name + "=" + (value || "") + expires + "; path=/";
}

// check if the cookie consent is already given
if (document.cookie.indexOf("cookieConsent=accepted") === -1) {
  // show the cookie consent banner
  document.querySelector(".cookie-consent-banner").setAttribute("data-cookie-consent", "false");
} else {
  // hide the cookie consent banner
  document.querySelector(".cookie-consent-banner").setAttribute("data-cookie-consent", "true");
}

// handle the cookie consent form submission
document.querySelector(".cookie-consent-banner__actions").addEventListener("submit", function (event) {
  event.preventDefault();
  var consentOption = document.querySelector('input[name="consent"]:checked').value;
  if (consentOption === "yes" || consentOption === "tokenize") {
    // set a cookie for 30 days
    setCookie("cookieConsent", "accepted", 30);
    // hide the cookie consent banner
    document.querySelector(".cookie-consent-banner").setAttribute("data-cookie-consent", "true");
  } else {
    // do nothing
  }
});

