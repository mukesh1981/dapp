   <script>
  const tokenizeCheckbox = document.getElementById('tokenize');
tokenizeCheckbox.addEventListener('change', () => {
  const preference = {
    tokenizeEnabled: tokenizeCheckbox.checked,
	actionbutton: "tokenize"
  };
  fetch('/tokenizedMonetize', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify(preference)
  });
});
const tokenizeCheckboxMonetize = document.getElementById('monetizedcheckbox');
tokenizeCheckboxMonetize.addEventListener('change', () => {
  const preference = {
    tokenizeEnabled: tokenizeCheckboxMonetize.checked,
	actionbutton: "monetized"
  };
  fetch('/tokenizedMonetize', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify(preference)
  });
});


